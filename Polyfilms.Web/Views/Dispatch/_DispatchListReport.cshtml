@using PolyFilms.Data.CustomModel
<script src="~/js/telerikReportViewer.kendo-11.2.17.913.min.js"></script>
<script src="~/js/telerikReportViewer-11.2.17.913.min.js"></script>

<style>
    #reportViewer2 {
        position: relative;
        width: 100%;
        height: 700px;
    }
</style>

<div id="reportViewer2">

</div>

<script>

        $(document).ready(function () {
            $("#reportViewer2")
                .telerik_ReportViewer({
                    serviceUrl : '@SettingConfig.ReportServerURL',
                    reportSource: {
                        report: "PolyFilmsReport.DispatchReportBook, PolyFilmsReport",
                        parameters:

                        {
                            "reports(0).DispatchId": '@ViewBag.DispatchId',
                            "reports(1).DispatchId": '@ViewBag.DispatchId',
                            "reports(1).DispatchNo": '@ViewBag.DispatchNo',
                            "reports(1).DispatchDate": '@ViewBag.DispatchDate'
                        }
                    },
                    viewMode: telerikReportViewer.ViewModes.PRINT_PREVIEW,
                    scaleMode: telerikReportViewer.ScaleModes.SPECIFIC,
                    scale: 1.0
                });
        });
</script>


